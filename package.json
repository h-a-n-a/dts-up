{
  "name": "dts-up",
  "version": "0.0.0",
  "description": "Rollup your d.ts files",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "esbuild src/index.ts --outdir=dist --bundle --sourcemap && tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/h-a-n-a/dts-up.git"
  },
  "keywords": [
    "typescript",
    "declaration",
    "rollup"
  ],
  "author": "hana <andywangsy@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/h-a-n-a/dts-up/issues"
  },
  "homepage": "https://github.com/h-a-n-a/dts-up#readme",
  "devDependencies": {
    "@hanao/eslint-config-basic": "^0.1.3",
    "@types/node": "^16.11.12",
    "esbuild": "^0.14.2",
    "eslint": "7.30.0"
  },
  "dependencies": {
    "typescript": "^4.5.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
